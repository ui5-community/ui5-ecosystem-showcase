{
  "name": "ui5-tsapp-simple",
  "version": "0.11.9",
  "description": "Simple showcase of a TypeScript setup for developing UI5 applications",
  "private": true,
  "author": "Peter Muessig",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf dist",
    "build": "ui5 build --clean-dest",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "dev": "ui5 serve --port 8080 -o index.html",
    "start": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "start-cdn": "ui5 serve  --port 8080 -o index-cdn.html --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint webapp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui5-community/ui5-ecosystem-showcase.git",
    "directory": "showcases/ui5-tsapp-simple"
  },
  "devDependencies": {
    "@types/openui5": "1.142.0",
    "@ui5/cli": "^4.0.37",
    "@ui5/ts-interface-generator": "^0.10.4",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "ui5-middleware-livereload": "workspace:^",
    "ui5-tooling-transpile": "workspace:^"
  }
}
