{
	"extends": "eslint:recommended",
	"env": {
		"es2022": true,
		"node": true,
		"jest": true,
		"mocha": true
	},
	"globals": {
		"SELECT": true,
		"INSERT": true,
		"UPSERT": true,
		"UPDATE": true,
		"DELETE": true,
		"CREATE": true,
		"DROP": true,
		"CDL": true,
		"CQL": true,
		"CXL": true,
		"cds": true
	},
	"rules": {
		"no-console": "off",
		"require-atomic-updates": "off"
	}
}
