{
  "sap.app": {
    "id": "ui5.bookshop",
    "type": "application",
    "title": "UI5 Bookshop",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "bookshop": {
        "uri": "/bookshop/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "northwind": {
        "uri": "/northwind/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "ui5.bookshop.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.118.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "models": {
      "": {
        "dataSource": "bookshop",
        "preload": true,
        "settings": {
          "groupId": "$auto",
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true
        }
      },
      "northwind": {
        "dataSource": "northwind",
        "preload": true,
        "settings": {
          "groupId": "$auto",
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true
        }
      }
    },
    "i18n": {
      "type": "sap.ui.model.resource.ResourceModel",
      "settings": {
        "bundleName": "ui5.bookshop.i18n.i18n"
      }
    }
  },
  "resources": {
    "css": [{
      "uri": "css/style.css"
    }]
  }
}
