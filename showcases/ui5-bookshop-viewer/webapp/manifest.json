{
  "sap.app": {
    "id": "ui5.bookshopviewer",
    "type": "application",
    "title": "UI5 Bookshop Viewer",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "bookshop": {
        "uri": "/bookshop/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "ui5.bookshopviewer.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.120.1",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "models": {
      "": {
        "dataSource": "bookshop",
        "preload": true,
        "settings": {
          "groupId": "$auto",
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ui5.bookshopviewer.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    }
  }
}
