{
  "name": "ui5-app",
  "version": "0.48.37",
  "description": "UI5 application using custom middlewares and tasks",
  "private": true,
  "author": "Volker Buzek, Peter Muessig",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui5-community/ui5-ecosystem-showcase.git",
    "directory": "showcases/ui5-app"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "prepare": "mkdirp dist",
    "build": "ui5 build --clean-dest",
    "build:sc": "ui5 build self-contained --clean-dest",
    "build:pwa": "ui5 build self-contained --clean-dest --config ui5-pwa.yaml",
    "start": "ui5 serve --config ui5-dist.yaml --port 1081 --open index.html",
    "start:ci": "ui5 serve --config ui5-min.yaml --port 1081",
    "start-cdn": "ui5 serve --config ui5-dist.yaml --port 1081 --open index-cdn.html",
    "dev": "ui5 serve --port 1081 --open index.html",
    "dev-h2": "ui5 serve --h2 --port 1082 --open index.html",
    "test": "karma start",
    "test:ci": "karma start karma-ci.conf.js",
    "test:ci-cov": "karma start karma-ci-cov.conf.js",
    "test:wdi5": "wdio webapp/test/e2e/wdio.conf.js",
    "watch:build": "npm-watch build",
    "watch": "npm-run-all prepare build --parallel watch:build start",
    "lint": "eslint webapp"
  },
  "watch": {
    "build": {
      "patterns": [
        "webapp"
      ],
      "extensions": "html,js,json,xml,properties",
      "quiet": false
    }
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "@octokit/core": "^4.2.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@stomp/stompjs": "^7.1.1",
    "@supabase/supabase-js": "^2.50.3",
    "@ui5/webcomponents": "^2.12.0",
    "@ui5/webcomponents-icons": "^2.12.0",
    "axios": "^1.11.0",
    "cmis": "^1.0.3",
    "easytimer.js": "^4.6.0",
    "fetch-mock": "^12.5.3",
    "firebase": "^11.10.0",
    "is-plain-object": "^5.0.0",
    "mypackage": "file:local/mypackage",
    "node-fetch": "^2.7.0",
    "pdfmake": "^0.2.20",
    "punycode": "^2.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "signalr": "^2.4.3",
    "tui-image-editor": "^3.15.3",
    "ui5-lib": "workspace:^",
    "ui5-module": "workspace:^",
    "xml-js": "^1.6.11",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@ui5/cli": "^4.0.19",
    "@wdio/cli": "^8",
    "@wdio/local-runner": "^8",
    "@wdio/mocha-framework": "^8",
    "@wdio/spec-reporter": "^8",
    "eslint": "^9.30.1",
    "eslint-plugin-jsdoc": "^51.3.3",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-wdio": "^8",
    "globals": "^16.3.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-ui5": "^4.1.0",
    "karma-ui5-transpile": "workspace:^",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "rimraf": "^6.0.1",
    "ui5-middleware-approuter": "workspace:^",
    "ui5-middleware-iasync": "workspace:^",
    "ui5-middleware-index": "workspace:^",
    "ui5-middleware-livereload": "workspace:^",
    "ui5-middleware-servestatic": "workspace:^",
    "ui5-middleware-simpleproxy": "workspace:^",
    "ui5-middleware-webjars": "workspace:^",
    "ui5-middleware-websocket": "workspace:^",
    "ui5-task-cachebuster": "workspace:^",
    "ui5-task-i18ncheck": "workspace:^",
    "ui5-task-minify-xml": "workspace:^",
    "ui5-task-pwa-enabler": "workspace:^",
    "ui5-task-zipper": "workspace:^",
    "ui5-tooling-less": "workspace:^",
    "ui5-tooling-modules": "workspace:^",
    "ui5-tooling-stringreplace": "workspace:^",
    "ui5-tooling-transpile": "workspace:^",
    "wdio-ui5-service": "^2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  }
}
