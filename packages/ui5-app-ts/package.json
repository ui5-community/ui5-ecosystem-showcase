{
  "name": "ui5-app-ts",
  "version": "0.3.8",
  "description": "Showcase of a TypeScript setup for developing UI5 applications",
  "private": true,
  "author": "Jorge Martins, Peter Muessig",
  "license": "Apache-2.0",
  "scripts": {
    "build": "ui5 build --clean-dest",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "dev": "ui5 serve --port 8080 -o index.html",
    "start": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "start-cdn": "ui5 serve  --port 8080 -o index-cdn.html --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint webapp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ui5-community/ui5-ecosystem-showcase.git",
    "directory": "packages/ui5-app-ts"
  },
  "devDependencies": {
    "@types/openui5": "^1.110.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@ui5/cli": "^2.14.17",
    "eslint": "^8.32.0",
    "typescript": "^4.9.4",
    "ui5-middleware-livereload": "^0.7.2",
    "ui5-tooling-modules": "^0.7.8",
    "ui5-tooling-transpile": "^0.3.5"
  },
  "ui5": {
    "dependencies": [
      "ui5-lib-ts",
      "ui5-middleware-livereload",
      "ui5-tooling-modules",
      "ui5-tooling-transpile"
    ]
  },
  "dependencies": {
    "ui5-lib-ts": "^0.1.0",
    "xlsx": "^0.18.5"
  }
}
