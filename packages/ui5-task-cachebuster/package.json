{
  "name": "ui5-task-cachebuster",
  "version": "2.0.1",
  "description": "Task for the UI5 tooling for simple cachebusting for standalone applications.",
  "author": "Hanna Olbert",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui5-community/ui5-ecosystem-showcase.git",
    "directory": "packages/ui5-task-cachebuster"
  },
  "scripts": {
    "lint": "eslint lib",
    "test": "ava"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/__assets__/**/*",
      "!test/__dist__/**/*"
    ],
    "verbose": true,
    "timeout": "5m"
  },
  "devDependencies": {
    "@ui5/cli": "^3.3.4",
    "ava": "^5.3.1"
  }
}
