{
  "tag": "{{tag}}",
  "library": "{{library}}",
  "designtime": "{{designtime}}",

  "namespace": "{{metadata.namespace}}",
  "interfaces": {{{json metadata.interfaces}}},

  "properties": {
  {{#each metadata.properties}}
{{{jsDoc @root.jsDoc "properties" @key}}}
    "{{@key}}": {{{json this}}},
  {{/each}}
  },

  "aggregations": {
  {{#each metadata.aggregations}}
{{{jsDoc @root.jsDoc "aggregations" @key}}}
    "{{@key}}": {{{json this}}},
  {{/each}}
  },

  "associations": {
  {{#each metadata.associations}}
{{{jsDoc @root.jsDoc "associations" @key}}}
    "{{@key}}": {{{json this}}},
  {{/each}}
  },

  "events": {
  {{#each metadata.events}}
{{{jsDoc @root.jsDoc "events" @key}}}
    "{{@key}}": {{{json this}}},
  {{/each}}
  },

  "getters": {{{json metadata.getters}}},
  "methods": {{{json metadata.methods}}}
}