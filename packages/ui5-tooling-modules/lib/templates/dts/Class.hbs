{{#each classes}}
{{#if this.superclass}}
declare module "{{{this._ui5QualifiedNameSlashes}}}" {

	/**
	 * Default imports
	 */
	import { default as WebComponentMetadata } from "sap/ui/core/webc/WebComponentMetadata";

	{{#each this._dts.imports}}
		{{#if @first}}

		/**
		 * Dynamic imports
		 */
		{{/if}}
		{{{generateImports @key this}}}

	{{/each}}

		export interface ${{{this.name}}}Settings extends ${{{this._dts.extends}}}Settings {
			{{#each this._dts.properties}}
				{{#if @first}}

				/**
				 * Properties
				 */

				{{/if}}
				{{#if this.description}}

				/**
				 * {{{this.description}}}
				 */
				{{/if}}
				{{{@key}}}?: {{{generatePropertySettings this.types}}}
			{{/each}}
			{{#each this._dts.aggregations}}
				{{#if @first}}

				/**
				 * Aggregations
				 */

				{{/if}}
				{{#if this.description}}

				/**
				 * {{{this.description}}}
				 */
				{{/if}}
				{{{@key}}}?: {{{generateAggregationSettings this.types}}}
			{{/each}}
			{{#each this._dts.associations}}
				{{#if @first}}

				/**
				 * Associations
				 */

				{{/if}}
				{{#if this.description}}

				/**
				 * {{{this.description}}}
				 */
				{{/if}}
				{{{@key}}}?: {{{generateAssociationSettings this.types}}}
			{{/each}}
			{{#each this._dts.events}}
				{{#if @first}}

				/**
				 * Events
				 */

				{{/if}}
				{{#if this.description}}

				/**
				 * {{{this.description}}}
				 */
				{{/if}}
				{{{@key}}}?: (oEvent: {{{generateEventSettings ../name @key}}}) => void
			{{/each}}
		}
		{{#each this._dts.events}}
			export interface {{{generateEventSettings ../name @key}}}Parameters {
				{{#each this.parameters}}
					{{#if this.dtsParamDescription}}

					/**
					 * {{{this.dtsParamDescription}}}
					 */
					{{/if}}
					{{{@key}}}?: {{{serializeTypeList this.types}}}
				{{/each}}
			}

			export type {{{generateEventSettings ../name @key}}} = Event<
				{{{generateEventSettings ../name @key}}}Parameters,
				{{{../name}}}
			>;
		{{/each}}

		export default class {{{this.name}}} extends {{{this._dts.extends}}}
	{{#if this._dts.implements}}
		implements {{join this._dts.implements}}
	{{/if}}
		{
			{{#each this._dts.implementsMarker}}
				{{{escapeInterfaceName this.package this.name}}}
			{{/each}}

			constructor(
				/**
				* Initial settings for the new control
				*/
				mSettings?: ${{{this.name}}}Settings
			);

			constructor(
				/**
				* ID for the new control, generated automatically if no ID is given
				*/
				id?: string,
				/**
				* Initial settings for the new control
				*/
				mSettings?: ${{{this.name}}}Settings
			);


			/**
			 * Creates a new subclass of class "{{{this._ui5QualifiedNameSlashes}}}" with name `sClassName` and enriches it with the information
			 * contained in `oClassInfo`.
			 *
			 * `oClassInfo` might contain the same kind of information as described in {@link sap.ui.core.Control.extend}.
			 *
			 *
			 * @returns Created class / constructor function
			 */
			static extend<T extends Record<string, unknown>>(
				/**
				 * Name of the class being created
				 */
				sClassName: string,
				/**
				 * Object literal with information about the class
				 */
				oClassInfo?: sap.ClassInfo<T, {{{this.name}}}>,
				/**
				 * Constructor function for the metadata object; if not given, it defaults to the metadata implementation
				 * used by this class
				 */
				FNMetaImpl?: Function
			): Function;

			/**
			 * Returns a metadata object for class "{{{this._ui5QualifiedNameSlashes}}}".
			 *
			 *
			 * @returns Metadata object describing this class
			 */
			static getMetadata(): WebComponentMetadata;
		}
}
{{/if}}
{{/each}}