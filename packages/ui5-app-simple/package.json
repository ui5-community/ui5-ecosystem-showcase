{
  "name": "ui5-app-simple",
  "version": "0.1.0",
  "description": "UI5 application using custom middlewares and tasks",
  "private": true,
  "author": "Volker Buzek, Peter Muessig",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui5-community/ui5-ecosystem-showcase.git",
    "directory": "packages/ui5-app-simple"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "mkdirp dist",
    "build": "ui5 build --clean-dest",
    "build:sc": "ui5 build self-contained --clean-dest",
    "start": "ui5 serve --config ui5-dist.yaml --port 1337 --open index.html",
    "dev": "ui5 serve --port 1337 --open index.html",
    "lint": "eslint webapp"
  },
  "dependencies": {
    "ui5-app-module": "^0.2.1"
  },
  "devDependencies": {
    "@ui5/cli": "^3.0.5",
    "mkdirp": "~2.1.3",
    "rimraf": "~4.1.2",
    "ui5-middleware-livereload": "^0.8.1",
    "ui5-tooling-modules": "^0.7.10",
    "ui5-tooling-transpile": "^0.4.0"
  }
}
