{
  "name": "ui5-app",
  "version": "0.16.0",
  "description": "UI5 application using custom middlewares and tasks",
  "private": true,
  "author": "Volker Buzek, Peter Muessig",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui5-community/ui5-ecosystem-showcase.git",
    "directory": "packages/ui5-app"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "mkdirp dist",
    "build": "ui5 build self-contained --clean-dest",
    "build:pwa": "ui5 build self-contained --clean-dest --config ui5-pwa.yaml --verbose",
    "start": "ui5 serve --config ui5-dist.yaml --port 1081 --open index.html",
    "start:ci": "ui5 serve --config ui5-min.yaml --port 1081",
    "dev": "ui5 serve --port 1081 --open index.html",
    "test": "karma start",
    "test:ci": "karma start karma-ci.conf.js",
    "test:wdi5": "wdio",
    "watch:build": "npm-watch build",
    "watch": "npm-run-all prepare build --parallel watch:build start",
    "lint": "eslint webapp"
  },
  "watch": {
    "build": {
      "patterns": [
        "webapp"
      ],
      "extensions": "html,js,json,xml,properties",
      "quiet": false
    }
  },
  "dependencies": {
    "tui-image-editor": "^3.15.3",
    "ui5-app-module": "^0.1.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@openui5/ts-types": "^1.103.0",
    "@ui5/cli": "^2.14.9",
    "@wdio/cli": "^7.20.5",
    "@wdio/local-runner": "^7.20.5",
    "@wdio/mocha-framework": "^7.20.3",
    "@wdio/spec-reporter": "^7.20.3",
    "@wdio/sync": "^7.20.5",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "chromedriver": "^103.0.0",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-wdio": "^7.19.4",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-ui5": "^2.4.0",
    "mkdirp": "~1.0.4",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "rimraf": "~3.0.2",
    "ui5-middleware-cfdestination": "^0.9.0",
    "ui5-middleware-iasync": "^1.0.0-alpha.0",
    "ui5-middleware-index": "^0.3.0",
    "ui5-middleware-livecompileless": "^0.3.0",
    "ui5-middleware-livereload": "^0.6.0",
    "ui5-middleware-livetranspile": "^0.4.0",
    "ui5-middleware-servestatic": "^0.4.0",
    "ui5-middleware-simpleproxy": "^0.9.0",
    "ui5-middleware-stringreplacer": "^0.4.0",
    "ui5-middleware-webjars": "^0.4.0",
    "ui5-task-compileless": "^0.3.0",
    "ui5-task-i18ncheck": "^0.3.9",
    "ui5-task-minify-xml": "^0.3.0",
    "ui5-task-pwa-enabler": "^0.3.0",
    "ui5-task-stringreplacer": "^0.7.0",
    "ui5-task-transpile": "^0.4.0",
    "ui5-task-zipper": "^0.5.0",
    "ui5-tooling-modules": "^0.4.0",
    "wdio-chromedriver-service": "^7.3.2",
    "wdio-ui5-service": "^0.9.5"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livecompileless",
      "ui5-middleware-cfdestination",
      "ui5-middleware-livereload",
      "ui5-middleware-iasync",
      "ui5-middleware-index",
      "ui5-middleware-livetranspile",
      "ui5-middleware-simpleproxy",
      "ui5-middleware-servestatic",
      "ui5-middleware-stringreplacer",
      "ui5-middleware-webjars",
      "ui5-task-compileless",
      "ui5-task-minify-xml",
      "ui5-task-i18ncheck",
      "ui5-task-pwa-enabler",
      "ui5-task-stringreplacer",
      "ui5-task-transpile",
      "ui5-task-zipper",
      "ui5-tooling-modules"
    ]
  }
}
