{
  "name": "ui5-app",
  "version": "0.24.3",
  "description": "UI5 application using custom middlewares and tasks",
  "private": true,
  "author": "Volker Buzek, Peter Muessig",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui5-community/ui5-ecosystem-showcase.git",
    "directory": "packages/ui5-app"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "mkdirp dist",
    "build": "ui5 build --clean-dest",
    "build:sc": "ui5 build self-contained --clean-dest",
    "build:pwa": "ui5 build self-contained --clean-dest --config ui5-pwa.yaml",
    "start": "ui5 serve --config ui5-dist.yaml --port 1081 --open index.html",
    "start:ci": "ui5 serve --config ui5-min.yaml --port 1081",
    "start-cdn": "ui5 serve --config ui5-dist.yaml --port 1081 --open index-cdn.html",
    "dev": "ui5 serve --port 1081 --open index.html",
    "dev-h2": "ui5 serve --h2 --port 1082 --open index.html",
    "test": "karma start",
    "test:ci": "karma start karma-ci.conf.js",
    "test:wdi5": "wdio",
    "watch:build": "npm-watch build",
    "watch": "npm-run-all prepare build --parallel watch:build start",
    "lint": "eslint webapp"
  },
  "watch": {
    "build": {
      "patterns": [
        "webapp"
      ],
      "extensions": "html,js,json,xml,properties",
      "quiet": false
    }
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.3",
    "@octokit/core": "4.2.0",
    "@supabase/supabase-js": "^2.4.1",
    "axios": "^1.2.4",
    "cmis": "^1.0.3",
    "firebase": "^9.17.2",
    "node-fetch": "^2.6.9",
    "tui-image-editor": "^3.15.3",
    "ui5-app-module": "^0.2.1",
    "ui5-lib": "^0.1.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@openui5/ts-types": "^1.110.0",
    "@ui5/cli": "^2.14.17",
    "@wdio/cli": "^7",
    "@wdio/local-runner": "^7",
    "@wdio/mocha-framework": "^7",
    "@wdio/spec-reporter": "^7",
    "@wdio/sync": "^7.27.0",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "chromedriver": "^111.0.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-wdio": "^7",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-ui5": "^2.4.0",
    "mkdirp": "~2.1.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "rimraf": "~4.1.2",
    "ui5-middleware-cfdestination": "^0.10.9",
    "ui5-middleware-iasync": "^1.0.0-alpha.2",
    "ui5-middleware-index": "^0.3.0",
    "ui5-middleware-livecompileless": "^0.3.2",
    "ui5-middleware-livereload": "^0.8.1",
    "ui5-middleware-servestatic": "^0.4.1",
    "ui5-middleware-simpleproxy": "^0.9.4",
    "ui5-middleware-webjars": "^0.4.2",
    "ui5-task-cachebuster": "^0.3.4",
    "ui5-task-compileless": "^0.3.2",
    "ui5-task-i18ncheck": "^0.3.10",
    "ui5-task-minify-xml": "^0.3.0",
    "ui5-task-pwa-enabler": "^0.3.3",
    "ui5-task-zipper": "^0.7.3",
    "ui5-tooling-modules": "^0.7.10",
    "ui5-tooling-stringreplace": "^0.1.0",
    "ui5-tooling-transpile": "^0.3.7",
    "wdio-chromedriver-service": "^8.0.1",
    "wdio-ui5-service": "^1.0.5"
  },
  "ui5": {
    "dependencies": [
      "ui5-lib",
      "ui5-middleware-livecompileless",
      "ui5-middleware-cfdestination",
      "ui5-middleware-livereload",
      "ui5-middleware-iasync",
      "ui5-middleware-index",
      "ui5-middleware-simpleproxy",
      "ui5-middleware-servestatic",
      "ui5-middleware-webjars",
      "ui5-task-compileless",
      "ui5-task-minify-xml",
      "ui5-task-i18ncheck",
      "ui5-task-pwa-enabler",
      "ui5-task-zipper",
      "ui5-task-cachebuster",
      "ui5-tooling-modules",
      "ui5-tooling-stringreplace",
      "ui5-tooling-transpile"
    ]
  }
}
